// WITH_DEFAULT_VALUE: false
public inline fun <T, R> with(receiver: T, f: T.() -> R): R = receiver.f()

class A(val a: Int) {
    fun A.foo(i: Int): Int {
        return i / 2
    }

    fun test() {
        A(1).foo(a + A(1).a)
    }
}

fun test() {
    val t = with(A(1)) {
        A(2).foo(a + A(2).a)
    }
}